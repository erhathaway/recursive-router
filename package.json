{
  "name": "recursive-router",
  "version": "0.0.1",
  "description": "A cross-platform application router. Declarative routing by way of layout primitives",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "BABEL_ENV=test jest --watch",
    "lint": "tslint --project ./tsconfig.json",
    "build": "rm -rf ./dist; BABEL_ENV=prod rollup -c",
    "watch": "BABEL_ENV=prod rollup -cw",
    "start": "watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erhathaway/recursive-router.git"
  },
  "keywords": [
    "router",
    "router-state",
    "router-management",
    "react",
    "react-state",
    "mobx",
    "redux",
    "primitives",
    "declarative-routing"
  ],
  "author": "Ethan Hathaway",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erhathaway/recursive-router/issues"
  },
  "homepage": "https://github.com/erhathaway/recursive-router#readme",
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "@types/query-string": "^6.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.82.0",
    "jest": "^23.1.0",
    "prettier": "^1.16.4",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "rollup-watch": "^4.3.1",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "query-string": "^5.1.1"
  },
  "peerDependencies": {}
