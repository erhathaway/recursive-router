{
  "env": {
    "prod": {
      "presets": [
        ["env", { "modules": false }]
      ],
      "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-object-rest-spread",
        "external-helpers",
        "transform-flow-strip-types"
      ]
     },
    "test": {
      "presets": [
        ["env", { "modules": false }]
      ],
      "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-flow-strip-types",
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}